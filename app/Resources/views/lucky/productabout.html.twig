{% extends 'basetemplate.html.twig' %}
{% block title %}{{product.productname}} list{% endblock %}

{% block content %}
	{% for cat in category %}
		<h2><a href="{{path('categorylist', {'slug': cat.id})}}">{{cat.namecat}}</a></h2>
	{% endfor %}
	<h3>{{product.productname}}</h3>
	<p>{{product.description}}</p>
	{{ form_start(form) }}
	{{ form_widget(form) }}
	{{ form_end(form) }}
{% endblock %}
